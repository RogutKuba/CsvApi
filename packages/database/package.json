{
  "name": "@starter/database",
  "version": "1.0.0",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "swc src --out-dir dist --source-maps true src",
    "build-tsup": "tsup",
    "type-check": "tsc",
    "lint": "eslint src",
    "push-m": "drizzle-kit push:mysql",
    "gen-migration": "drizzle-kit generate:mysql",
    "run-migration": "tsx src/migrate.ts",
    "drop-migration": "drizzle-kit drop",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@planetscale/database": "^1.11.0",
    "@starter/base": "workspace:*",
    "@starter/eslint-config-custom": "workspace:*",
    "@starter/tsconfig": "workspace:*",
    "@t3-oss/env-core": "^0.7.1",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.1",
    "drizzle-orm": "^0.29.1",
    "tsx": "^3.14.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101"
  }
}
